package pageObjects;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class ContactUsPage extends BasePage{
	
	public ContactUsPage(WebDriver driver)
	{ 
		super(driver);
	}
	
	
	@FindBy(xpath="//*[text()='Get In Touch']")WebElement msg_getIntouch;
	@FindBy(xpath="//input[@name='name']")WebElement name;
	@FindBy(xpath="//input[@name='email']")WebElement email;
	@FindBy(xpath="//input[@name='subject']")WebElement subject;
	@FindBy(xpath="//textarea[@name='message']")WebElement message;
	@FindBy(xpath="//input[@name='upload_file']")WebElement btn_uploadFile;
	@FindBy(xpath="//input[@name='submit']")WebElement btn_submit;
}
